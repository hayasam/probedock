.testKeyGenerator
  h4
    margin-top 0
    font-weight bold
  form
    #keyNumber
      width 5em
    #keyProject
      input, .btn
        width 18em
  .key
    display block
    float left
    margin-right 0.4em
    margin-top 0.6em

.test-key-label
  font-family Monaco, Monospace
  font-size 1em
  padding 0.1em 0.6em
  border 1px solid #333333
  border-radius 3px
  background-color #eeeeee
  color #222222
  &.copied
    background-color #cccccc
    color #888888
