.form-group
  label for='user-choice' Filter by user
  ui-select#user-choice ng-if='modelProperty' ng-model='parentData[modelProperty]' multiple=true reset-search-input=true
    ui-select-match placeholder='All users' {{ $item.name }}
    ui-select-choices repeat='user.id as user in userChoices | filter: $select.search'
      span ng-bind-html='user.name'
